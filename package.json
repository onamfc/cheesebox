{
  "name": "cheesebox",
  "version": "1.6.3",
  "description": "Secure video sharing platform with HLS streaming and AWS S3 storage",
  "author": "Cheesebox Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/onamfc/cheesebox.git"
  },
  "keywords": [
    "video",
    "sharing",
    "hls",
    "aws",
    "s3",
    "mediaconvert",
    "nextjs",
    "encryption",
    "secure"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest src/__tests__/security",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "prisma generate",
    "clean": "rm -rf .next node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "check": "npm run type-check && npm run lint",
    "security-check": "npm run test:security && npm run type-check"
  },
  "dependencies": {
    "@aws-sdk/client-mediaconvert": "^3.958.0",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/client-ses": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@lobehub/icons": "^4.0.2",
    "@onamfc/developer-log": "^1.2.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "bcryptjs": "^3.0.3",
    "csrf": "^3.1.0",
    "expo-server-sdk": "^4.0.0",
    "hls.js": "^1.6.15",
    "jsonwebtoken": "^9.0.3",
    "next": "16.1.1",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "resend": "^6.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/hls.js": "^0.13.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "jest": "^30.2.0",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
